{
  "name": "quake-parser",
  "version": "1.0.0",
  "description": "A project to parse the Quake log file.",
  "main": "index.js",
  "author": "William Giani <holtzs.william@gmail.com>",
  "license": "MIT",
  "scripts": {
    "quick-compile": "ts-node src/index.ts",
    "generate": "node ./dist/index.js generate -i ./input/game.log -o ./output/out.json",
    "dev": "nodemon src/index.ts",
    "build": "tsc ./src/index.ts --outDir ./dist --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^17.0.38",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "yargs": "^17.5.1"
  }
}